// WE DO NOT SUPPORT METHODS YET

@empty @runtime
print: (String) -> ()


type Option(a) {
    Some(a)
    None
}

methods for Option(a) {
    map: ((a) -> b) -> Option(b) 
    map f = this match {
        Some(a) => Some(f(a))
        None => None
    }
}

retConst x = "world"

main {
    let a = Option.Some("hello").map(retConst)
    a match {
        Some("world") => print("it is world")
        _ => print("anything else")
    }
}

